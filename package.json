{
  "name": "@selenic/core",
  "version": "0.10.0",
  "description": "selenic core",
  "keywords": [
    "license"
  ],
  "homepage": "https://github.com/mkwtys/selenic",
  "bugs": "https://github.com/mkwtys/selenic/issues",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/mkwtys/selenic.git"
  },
  "license": "MIT",
  "author": "mkwtys <mkwtys@gmail.com>",
  "files": [
    "lib",
    "!lib/__tests__"
  ],
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "build": "tsc",
    "clean": "rimraf lib/",
    "coverage": "jest --coverage",
    "test": "jest",
    "test:update": "jest -u"
  },
  "babel": {
    "presets": [
      "@babel/preset-typescript",
      [
        "@babel/preset-env",
        {
          "targets": {
            "node": 10
          }
        }
      ]
    ]
  },
  "prettier": {
    "printWidth": 120,
    "semi": false,
    "singleQuote": true
  },
  "jest": {
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "fixtures"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "fixtures"
    ]
  },
  "dependencies": {
    "semver": "^7.0.0"
  },
  "devDependencies": {
    "@babel/preset-env": "7.9.6",
    "@babel/preset-typescript": "7.9.0",
    "@types/jest": "25.2.1",
    "@types/node": "12.12.37",
    "@types/semver": "7.1.0",
    "jest": "25.5.3",
    "rimraf": "3.0.2",
    "typescript": "3.8.3"
  },
  "publishConfig": {
    "access": "public"
  }
}
